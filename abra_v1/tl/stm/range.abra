def +: (self: Int, other: Int) -> Int = llvm {
  %1 = add nsw i32 %self, %other
  ret i32 %1
}

def <: (self: Int, other: Int) -> Boolean = llvm {
  %1 = icmp slt i32 %self, %other
  ret i1 %1
}

def times = { self: Int, callback: () -> Unit ->
    var i = 0
    while i < self {
        callback()
        i = i + 1
    }
}: Unit