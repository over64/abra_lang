def +: (self: Int, other: Int) -> Int = llvm {
  %1 = add nsw i32 %self, %other
  ret i32 %1
}

def foo = \fn: () -> Unit ->
    fn()

def main = {
    var x = 0

    #store and call
    val closure = {
        x = x + 1
    }
    closure()

    # anon call
    {
        x = x + 1
    }()

    foo(closure)

    foo({
        x = x + 1
    })

    x
}: Int # 4 expected